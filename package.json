{
  "name": "anoa-cli",
  "version": "0.3.0-rc1",
  "description": "React Native Typescript Boilerplate & Code Generator CLI",
  "private": false,
  "author": "Budi Adiono <budiadiono@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anoaland/anoa-cli.git"
  },
  "keywords": [
    "cli",
    "react-native",
    "typescript",
    "boilerplate",
    "scaffolder"
  ],
  "bin": {
    "anoa": "bin/anoa"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,ts,tsx,json}",
    "test": "ava",
    "watch": "ava --watch",
    "coverage": "nyc ava",
    "build": "rm -rf dist && tsc && cp -R ./src/templates ./dist/templates && cp -R ./src/assets ./dist/assets"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "gluegun": "next",
    "jsx-ast-utils": "^2.0.1",
    "prettier": "^1.14.3",
    "ramda": "^0.25.0",
    "ts-simple-ast": "^18.1.0",
    "typescript": "^2.5.3"
  },
  "devDependencies": {
    "@types/node": "^8.0.47",
    "@types/prettier": "^1.13.2",
    "@types/ramda": "types/npm-ramda#dist",
    "ava": "^0.22.0",
    "nyc": "^11.2.1"
  }
}
