type <%= props.name %>Actions = <% props.actions.forEach(function(s) { %>
  | {
      type: '<%- props.upperCase(props.name) %>/<%- props.upperCase(s.name) %>'
      <% if (s.payload) { %>payload<% if (s.optional) { %>?<% } %>: <%- s.payload %><% } %>
    }<% }) %>

export default <%= props.name %>Actions